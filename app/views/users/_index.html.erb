<table class="table table-hover table-inverse">
      <thread>
          <tr>
          　　<th colspan="1"></th>
              <th colspan="1"></th>
              <th>User id</th>
              <th>User Image</th>
              <th>Name</th>
              <th>Introduction</th>
              <th>User page</th>
              <th colspan="1"></th>
              
          </tr>
      </thread>
      <tbody>
      <% users.each do |user| %>
       <tr>
        <th colspan="1"></th>
        <th colspan="1"></th>
        <th><%= "#{user.id}" %></th>
        <th><%= link_to user_path(user) do %>
            <%= attachment_image_tag user, :profile_image, size: '40x40', fallback: "no_image.jpg"%>
            <% end %></th>
        <th><%= user.name %></th>
        <th><%= user.introduction %></th>
        <th><%= link_to "Show", user_path(user.id) %></th>
        <th colspan="1"></th>
        </tr>
      <% end %>
    　</tbody>
</table>


