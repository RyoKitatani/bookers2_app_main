<% if user_signed_in? %>
<div class="text-center mb-3">
    <%= form_with url: search_path, class: "search-form", method: :get, local: true do |f| %>
    <%= f.text_field :content %>
    <%= select_tag 'model', options_for_select({ "User" => "user", "Book" => "book" }) %>
    <%= select_tag 'method', options_for_select({ "完全一致" => "perfect", "前方一致" => "forward","後方一致" => "backward", "部分一致" => "partial" }) %>
    <%= f.submit :"検索する" %>
    <% end %>
<% end%>